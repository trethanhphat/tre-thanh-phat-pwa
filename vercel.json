{
  "version": 2,
  "functions": {
    "api/news/image-proxy/route.ts": {
      "runtime": "edge",
      "maxDuration": 10,
      "regions": ["hkg1", "sin1"],
      "cache": {
        "maxAge": 604800,
        "staleWhileRevalidate": 86400
      }
    }
  },
  "routes": [
    {
      "src": "/api/news/image-proxy",
      "headers": {
        "Cache-Control": "public, s-maxage=604800, stale-while-revalidate=86400"
      }
    }
  ]
}
