{
  "version": 2,
  "functions": {
    "api/image-proxy/route.ts": {
      "runtime": "edge",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/image-proxy",
      "dest": "/api/image-proxy/route.ts",
      "headers": {
        "Cache-Control": "public, s-maxage=604800, stale-while-revalidate=86400"
      }
    }
  ]
}
