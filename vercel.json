{
  "version": 2,
  "functions": {
    "api/image-proxy/route.ts": {
      "runtime": "edge",
      "maxDuration": 10,
      "cache": {
        "maxAge": 604800,
        "staleWhileRevalidate": 86400
      }
    }
  },
  "routes": [
    {
      "src": "/api/image-proxy",
      "dest": "/api/image-proxy/route.ts"
    }
  ]
}
